<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.erox.reversi"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="25" />

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.erox.reversi.MainActivity"
            android:icon="@drawable/riven"
            android:label="PRINCIPAL" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.erox.reversi.HelpActivity"
            android:icon="@drawable/ahri"
            android:label="AYUDA" />
        <activity
            android:name="com.example.erox.reversi.ConfigActivity"
            android:icon="@drawable/set"
            android:label="CONFIGURACION" />
        <activity
            android:name="com.example.erox.reversi.DesarrolloActivity"
            android:icon="@drawable/set"
            android:label="DESARROLLO" />
        <activity
            android:name="com.example.erox.reversi.ResultsActivity"
            android:icon="@drawable/set"
            android:label="RESULTADOS" >
        </activity>

        <provider
            android:name="com.android.tools.fd.runtime.InstantRunContentProvider"
            android:authorities="com.example.erox.reversi.com.android.tools.fd.runtime.InstantRunContentProvider"
            android:multiprocess="true" />
    </application>

</manifest>